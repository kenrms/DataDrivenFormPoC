@GetQuestionFragmentToRender()

@foreach (var childForm in Question.Options.Select(option => option.ChildForm))
{
    @if (childForm != null)
    {
        <QuestionListComponent IsChildForm=true
                               IsHidden=ShouldHideQuestionList(childForm)
                               Questions=childForm.Questions.ToList()
                               ResponsesMap=ResponsesMap
                               ValidationMessagesMap=ValidationMessagesMap />
    }

}